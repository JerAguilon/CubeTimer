package cubetimer;

public abstract class Scrambler {
	static String scramble = "";
	
	public Scrambler(){
		setScramble();
	}

	abstract protected String[] scramble();
	
	public String getScramble(){
		return scramble;
	}
	public void setScramble(){
		String[] scr = scramble();
		scramble = "";
		for(String item : scr){
			scramble += item  + " ";
		}
	}
}
